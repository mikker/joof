{
  "name": "joof",
  "version": "0.3.2",
  "description": "Add custom JavaScript or CSS to any webpage",
  "bin": "cli.js",
  "scripts": {
    "start": "node cli.js serve",
    "dev": "nodemon --exec 'npm start'",
    "setup": "node cli.js setup",
    "test": "tape test/**/*_test.js | tap-spec"
  },
  "keywords": [
    "js",
    "css",
    "browser-extension",
    "customization",
    "macos",
    "safari"
  ],
  "author": "Mikkel Malmberg <mikkel@brnbw.com> (https://mikkelmalmberg.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikker/joof.git"
  },
  "homepage": "https://github.com/mikker/joof",
  "devDependencies": {
    "got": "^9.0.0",
    "nodemon": "^1.14.10",
    "tape": "^4.9.0",
    "tap-spec": "^5.0.0"
  },
  "dependencies": {
    "execa": "^1.0.0",
    "listr": "^0.14.0",
    "meow": "^5.0.0",
    "sudo-prompt": "^8.1.0",
    "user-home": "^2.0.0"
  }
}
